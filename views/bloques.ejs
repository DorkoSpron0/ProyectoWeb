<%- include('partials/main') %>

<div class="container">
<% for(var i = 0; i < bloques.length; i++) {%>
    <div class="card py-2">
        <div class="row">
            <div class="col-md-4">
                <img src="<%= bloques[i].img%>" class="img-fluid rounded">
            </div>
            <div class="col-md-8">
                <h1 class="card-header text-center"><%= bloques[i].title %></h1>
                <div class="card-body">
                    <p><%= bloques[i].description%></p>
                </div>
            </div>
        </div>
    </div>
    
    
<%}%>
</div>